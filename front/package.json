{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "env": "ts-node src/commands/validateEnv.ts",
    "dev": "npm run env && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "reset": "ts-node src/commands/resetNext.ts",
    "code": "tsc --noEmit --skipLibCheck --incremental false",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "format": "prettier -w .",
    "test": "jest --forceExit --detectOpenHandles --watch",
    "format:check": "prettier -c .",
    "deploy": "next-sitemap --config next-sitemap.config.js"
  },
  "dependencies": {
    "@nextui-org/react": "^2.1.13",
    "@types/node": "20.6.2",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "axios": "^1.5.0",
    "clsx": "^2.0.0",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.3.1",
    "formik": "^2.4.5",
    "next": "13.4.19",
    "next-auth": "^4.23.1",
    "next-themes": "^0.2.1",
    "postcss": "8.4.29",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-icons": "^4.11.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react-google-recaptcha": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "envalid": "^7.3.1",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next-router-mock": "^0.9.9",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1"
  }
}
