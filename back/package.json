{
  "name": "back-end",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "code": "tsc --noEmit --skipLibCheck --incremental false && tsc-alias",
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias --skipLibCheck ",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "seed": "knex seed:run --knexfile=knexfile.ts --verbose",
    "migrate": "knex migrate:latest --knexfile=knexfile.ts --verbose",
    "rollback": "knex migrate:rollback --knexfile=knexfile.ts --verbose",
    "make:seeder": "knex seed:make --knexfile=knexfile.ts -x ts --env development",
    "make:migration": "knex migrate:make --knexfile=knexfile.ts -x ts --env development",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.85",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.3",
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.6.0",
    "@types/nodemailer": "^6.4.10",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.0.0",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.5.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.9.5",
    "nodemon": "^3.0.1",
    "objection": "^3.1.1",
    "pg": "^8.11.3",
    "pm2": "^5.3.0",
    "reflect-metadata": "^0.1.13",
    "stripe": "^13.6.0",
    "supertest": "^6.3.3",
    "typedi": "^0.10.0",
    "uuid": "^9.0.1",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "yup": "^1.2.0"
  }
}
